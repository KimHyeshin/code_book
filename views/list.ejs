<table class="table mt-2">
        <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">영문명</th>
            <th scope="col">한글명</th>
            <th scope="col">Date</th>
            <th scope="col">Author</th>
            <th scope="col">수정/삭제</th>
        </tr>
        </thead>
        <tbody>
        <% for(var i=0; i<data.length;i++) {%>
            <tr>
                <th scope="row"><%= i+1 %></th>
                <td><%= data[i].keywordEng %></td>
                <td><%= data[i].keywordKor %></td>
                <td><%= data[i].date %></td>
                <td><%= data[i].author %></td>
                <td><button type="button" id="modifyBtn" class="btn btn-outline-dark btn-sm">수정</button> <button type="button" id="deleteBtn" class="btn btn-outline-dark btn-sm" data-keyword="<%= data[i].keywordEng %>">삭제</button></td>
            </tr>
        <% }%>
        </tbody>
    </table>
